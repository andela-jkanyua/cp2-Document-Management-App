{
  "name": "document-management_app",
  "version": "1.0.0",
  "description": "A document management app with users and roles",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "prestart": "./node_modules/.bin/sequelize db:migrate && node server/startMessage",
    "start": "node ./server/server.js",
    "lint": "node_modules/.bin/esw webpack.config.* client server",
    "lint:watch": "npm run lint -- --watch",
    "test-server": "NODE_ENV=test && sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all && ./node_modules/.bin/istanbul cover _mocha -- -R spec --require babel-register --timeout 20000 \"test/server/**/*.js\"",
    "test-client": "mocha --reporter spec test/client/setup.js \"test/client/**/*.js\"",
    "test": "npm-run-all --parallel test-client test-server",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andela-jkanyua/cp2-Document-Management-App.git"
  },
  "keywords": [
    "app",
    "document",
    "checkpoint",
    "management"
  ],
  "author": "Jimnah Magira",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-jkanyua/cp2-Document-Management-App/issues"
  },
  "homepage": "https://github.com/andela-jkanyua/cp2-Document-Management-App#readme",
